#!groovy

pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
    timestamps()
    }
    stages {
      stage("Stage1"){
        steps {
          echo '==========Building==========='
          sh 'docker build -t ipscript2 .'
          }
      }
      stage("Stage2"){
        steps {
          echo '==========Running============'
          sh 'docker run -d ipscript2'
        }
      }
    }
}
